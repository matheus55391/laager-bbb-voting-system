        "build:web": "nx build web",
        "test": "nx test api-gateway",
        "test:watch": "nx test api-gateway --watch",
        "test:cov": "nx test api-gateway --coverage",
        "test:e2e": "nx e2e api-gateway-e2e",
        "lint": "nx lint api-gateway",
        "lint:fix": "nx lint api-gateway --fix",
        "format": "prettier --write \"apps/**/*.ts\"",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f"
