{
    "name": "@laager-bbb-voting-system/aggregate-service-e2e",
    "version": "0.0.1",
    "private": true,
    "nx": {
        "implicitDependencies": [
            "@laager-bbb-voting-system/aggregate-service"
        ],
        "targets": {
            "e2e": {
                "executor": "@nx/jest:jest",
                "outputs": [
                    "{projectRoot}/test-output/jest/coverage"
                ],
                "options": {
                    "jestConfig": "apps/api/e2e/aggregate-service-e2e/jest.config.ts",
                    "passWithNoTests": true
                },
                "dependsOn": [
                    "@laager-bbb-voting-system/aggregate-service:build",
                    "@laager-bbb-voting-system/aggregate-service:serve"
                ]
            }
        }
    }
}
